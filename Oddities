#include <stdio.h>
#include <stdlib.h>

int main (){
int buff;
int a;
int index = 0; // 0 -> 1, 1 -> 2, 2 -> 3

scanf("%d", &buff);

while (buff != index){
if(scanf("%d", &a) == 1){
    if ((a % 2) == 0){  
        printf("%d is even\n", a);
        index++;
    }
    else if ((a% 2) != 0){
        printf ("%d is odd\n", a);
        index++;
    }
    
    
}
}
return 0;
}
